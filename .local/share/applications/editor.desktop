[Desktop Entry]
Type=Application
Name=Signal (start in tray)
Comment=Signal - Private Messenger
Comment[de]=Signal - Sicherer Messenger
Icon=signal-desktop
Terminal=false
Categories=Network;InstantMessaging;
StartupWMClass=Signal
